{
  "request": {
    "method": "POST",
    "urlPathPattern": "/orgs/orange/base_products/products/search",
    "bodyPatterns": [
      {
        "matchesJsonPath": "$[?(@.name =~ /^[iI][pP][hH][oO][nN][eE](\\s*13)?$/)]"
      }
    ]
  },
  "response": {
    "status": 200,
    "bodyFileName": "product/iphone13.json",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}