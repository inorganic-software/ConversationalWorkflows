{
  "request": {
    "method": "POST",
    "urlPathPattern": "/orgs/orange/base_products/products/search",
    "bodyPatterns": [
      {
        "matchesJsonPath": "$[?(@.name =~ /^[nN][oO][kK][iI][aA](\\s+[xX][rR]20)?$/)]"
      }
    ]
  },
  "response": {
    "status": 200,
    "bodyFileName": "product/nokia-xr20.json",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
